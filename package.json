{
  "name": "codiac-front-end",
  "version": "1.0.2-17",
  "scripts": {
    "local": "ng s --configuration=local",
    "s": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng s --poll 2000",
    "build": "ng build",
    "stamp-version": "node --experimental-json-modules --experimental-modules  ./devops/stamp-version.mjs",
    "dev-build": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build  -c prod",
    "release-build": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build  -c prod",
    "prod-build": "npm run build:ssr",
    "release-server": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng s -c prod --port 3000",
    "prod-server": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng s -c prod --port 3000",
    "copy-app-config": "cp ./src/app-config.json ./dist",
    "build-and-publish": "codiac build -r && codiac publish",
    "build-and-deploy": "npm run build-and-publish && codiac deploy",
    "win-copy-deploy-assets": "Xcopy /i /e /y deploy-assets dist\\",
    "lnx-copy-deploy-assets": "cp -avr deploy-assets dist",
    "azWhereIsCluster": "az aks list -o table",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "show-events": "kubectl get events --all-namespaces",
    "show-cluster-issuer": "kubectl get ClusterIssuer --all-namespaces",
    "azSetSubcription": "az account set --subscription 37dbb924-5ee5-470b-b2a3-187e9dea7008",
    "pointAKS": "az aks get-credentials -n demo-cluster-01 -g demo-cluster-01-rg --overwrite-existing",
    "k8sSwitch": "kubectl config use-context demo-cluster-01",
    "set-cluster": "npm run azSetSubcription && npm run pointAKS && npm run k8sSwitch",
    "pointAKS2": "az aks get-credentials -n demo-cluster-01 -g demo-cluster-01-rg --overwrite-existing",
    "k8sSwitch2": "kubectl config use-context demo-cluster-02",
    "set-cluster2": "npm run azSetSubcription && npm run pointAKS2 && npm run k8sSwitch2",
    "build-svg-sprites": "svg-sprite --config svg-sprite-config.json src/assets/icons/core-source/*.svg",
    "proj-login": "az login && npm run set-cluster && cod login",
    "dev:ssr": "ng run angular:serve-ssr",
    "serve:ssr": "node dist/angular/server/main.js",
    "build:ssr": "ng build -c prod && ng run angular:server:production",
    "prerender": "ng run angular:prerender",
    "cluster-create": "codiac noc:cluster:create demo-cluster-uk-3 -p codemo -s 37dbb924-5ee5-470b-b2a3-187e9dea7008 -g 'demo-cluster-uk-3-rg' -l 'UK West' -n Standard_DS2_v2 -q 2"
  },
  "private": true,
